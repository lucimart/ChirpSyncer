{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^http://192.168"
    },
    {
      "pattern": "^https://github.com/lucimart/ChirpSyncer/(releases|compare)"
    },
    {
      "pattern": "^https://github.com/lucimart/ChirpSyncer/(blob|tree)/main"
    },
    {
      "pattern": "^https://lucimart.github.io/ChirpSyncer"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
